{
  "name": "obsidian-claude-integration",
  "version": "1.0.0",
  "description": "Chat with Claude AI in Obsidian. Claude can read, edit, and create files in your vault with intelligent file operations and automatic error handling.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": ["obsidian", "plugin", "claude", "ai", "anthropic", "chat", "assistant"],
  "author": "bodfather",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bodfather/obsidian-claude-plugin.git"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
